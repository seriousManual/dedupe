{
  "name": "dedupe",
  "author": "Manuel Ernst <seriousstuff@gmx.de>",
  "license": "MIT",
  "description": "easy deduplication of array values",
  "version": "4.0.2",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/seriousManual/dedupe.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepublish": "npm run build",
    "test": "jest",
    "build": "microbundle --no-sourcemap"
  },
